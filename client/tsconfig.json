{
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "outDir": "build",
    "target": "ES2017",
    "jsx": "react",
    "module": "ESNext",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "types": [
        "vite/client",
        "node"
    ],
    "typeRoots": ["./node_modules", "node_modules/@types"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}